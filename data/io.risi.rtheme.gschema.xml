<?xml version="1.0" encoding="UTF-8"?>
<schemalist>
  <schema path="/io/risi/rtheme/" id="io.risi.rtheme">
    <key type="s" name="theme-name">
      <default>"risi"</default>
      <summary>rtheme name</summary>
      <description>
        The basename of the default theme applied by rtheme.
      </description>
    </key>
    <key type="s" name="variant-name">
      <default>"main"</default>
      <summary>rtheme variant</summary>
      <description>
        The name of the variant applied by rtheme.
      </description>
    </key>
    <key type="as" name="enabled-plugins">
      <default>["gtk3", "gtk4"]</default>
      <summary>Enabled plugins</summary>
      <description>
        The list of enabled plugins.
      </description>
    </key>
  </schema>
</schemalist>